<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ImageSelectView">
        <attr name="divide_index" format="integer" />
        <attr name="default_image" format="reference" />
        <attr name="image_txt1" format="string" />
        <attr name="image_txt2" format="string" />
        <attr name="image_max_size" format="integer" />
        <attr name="leftMargin" format="integer" />
        <attr name="rightMargin" format="integer" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <!--选中时text的颜色-->
        <attr name="selectedTabTextColor" format="color" />
        <attr name="selectedTabBackground" format="reference" />
        <attr name="unselectedTabBackground" format="reference" />
        <!--选中时底部线的颜色-->
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsIndicatorWidth" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="pststabSelectTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PswInputView">
        <attr name="count" format="integer"/>
        <attr name="border_color" format="color"/>
        <attr name="dot_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="EditView">
        <attr name="et_maxLength" format="integer"/>
    </declare-styleable>

</resources>